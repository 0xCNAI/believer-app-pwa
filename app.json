{
  "expo": {
    "name": "believer-web",
    "slug": "believer-web",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/logo.jpg",
    "scheme": "believerweb",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images/android-icon-foreground.png",
        "backgroundImage": "./assets/images/android-icon-background.png",
        "monochromeImage": "./assets/images/android-icon-monochrome.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/logo.jpg",
      "display": "standalone",
      "backgroundColor": "#18181b",
      "startUrl": "/",
      "themeColor": "#18181b",
      "name": "Believer",
      "shortName": "Believer",
      "lang": "zh-TW",
      "meta": {
        "description": "Believer - Your Crypto Compass",
        "og:description": "Believer - Your Crypto Compass"
      }
    },
    "plugins": [
      "expo-router",
      "expo-font"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": false
    }
  }
}